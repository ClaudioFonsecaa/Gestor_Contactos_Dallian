<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Gestor_Contactos_Dallian.MainPage"
            xmlns:local="clr-namespace:Gestor_Contactos_Dallian"
            NavigationPage.HasNavigationBar="False"
            BarBackgroundColor="Black"
            SelectedTabColor="Goldenrod"
            UnselectedTabColor="White"
            xmlns:android="clr-namespace:Xamarin.Forms.PlatformConfiguration.AndroidSpecific;assembly=Xamarin.Forms.Core"
            android:TabbedPage.ToolbarPlacement="Bottom"
            >

        <local:HomePage Title="Home" IconImageSource="home_icon"/>
        <local:AdicionarPage Title="Adicionar" IconImageSource="plus_icon"/>
        <local:ProcurarPage Title="Procurar" IconImageSource="search_icon"/>
    

</TabbedPage>
